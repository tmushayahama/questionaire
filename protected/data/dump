CREATE TABLE `que_user_question` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `parent_id` int(11) DEFAULT NULL,
  `questionnaire_id` int(11) NOT NULL,
  `content` varchar(528) DEFAULT NULL,
  `scale` int(11) DEFAULT NULL,
  `answer` varchar(500) DEFAULT NULL,
  `order` int(11) NOT NULL DEFAULT '1',
  `status` int(11) NOT NULL DEFAULT '0',
  PRIMARY KEY (`id`),
  KEY `user_question_parent_id` (`parent_id`),
  KEY `user_question_questionnaire_id` (`questionnaire_id`),
  CONSTRAINT `user_question_questionnaire_id` FOREIGN KEY (`questionnaire_id`) REFERENCES `que_questionnaire` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `user_question_parent_id` FOREIGN KEY (`parent_id`) REFERENCES `que_question_bank` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=22 DEFAULT CHARSET=utf8;
